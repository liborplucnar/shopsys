{# @var requiredSettingsMessages string[] #}
<div class="in-message in-message--warning in-message--block">
    <ul class="in-message__list">
        {% for message in requiredSettingsMessages %}
            <li class="in-message__list__item">
                {{ icon('warning') }}
                {{ message|raw }}
            </li>
        {% endfor %}
    </ul>
</div>
