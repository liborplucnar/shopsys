{
    "name": "shopsys/admin-bundle",
    "type": "library",
    "description": "Shopsys Platform administration",
    "keywords": ["admin", "sonata", "bundle", "Shopsys Platform", "SSFW", "SSP"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\AdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\AdminBundle\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "sonata-project/admin-bundle": "^4.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "shopsys/coding-standards": "14.0.x-dev"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
